{
    "manifest_version": 2,
    "name": "Hello World",
    "version": "1.0",
    "description": "A hello world extensions!",
    "background": {
        "scripts": ["js/background.js"],
        "persistent": false
    },
    "icons": {
        "128": "img/icon128.png",
        "48": "img/icon48.png",
        "16": "img/icon16.png"
    },
    "browser_action": {
        "default_icon": "img/icon16.png",
        "default_popup": "img/popup.html"
    },
    "options_page": "html/options.html",
    "permissions": ["storage", "contextMenus"]
}
