{
    "manifest_version": 2,
    "name": "Real Word",
    "version": "1.0",
    "description": "영단어, 숙어 암기를 도와주는 크롬 확장 프로그램입니다.",
    "background": {
        "scripts": ["js/background.js"],
        "persistent": false
    },
    "icons": {
        "128": "img/icon128.png",
        "48": "img/icon48.png",
        "16": "img/icon16.png"
    },
    "browser_action": {
        "default_icon": "img/icon16.png",
        "default_popup": "html/popup.html"
    },
    "options_page": "html/options.html",
    "permissions": ["storage", "contextMenus"]
}
